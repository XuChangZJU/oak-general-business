<!-- index.wxml -->
<view class="qrcode_view">
    <view class="col">
        <block wx:if="{{oakLoading}}">
            <l-loading show="{{true}}" type="circle"></l-loading>
        </block>
        <block wx:else>
            <image src="{{url}}" mode="aspectFit" class="image" />
        </block>
    </view>
</view>